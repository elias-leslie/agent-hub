  "overview": {
    "summary": "Agent Hub is a unified agentic AI service that all projects connect to for Claude/Gemini workloads. It consolidates scattered agent implementations into a single, project-agnostic service with centralized credential management, automatic fallback, and full observability.",
    "current_state": "Multiple duplicate Claude/Gemini client implementations exist across summitflow and portfolio-ai with hardcoded paths, project-specific logic, and inconsistent error handling. The DualProviderClient pattern exists but is not centralized.",
    "desired_state": "A standalone service with its own database, REST + WebSocket APIs, and a management UI. Any project can call Agent Hub to get AI completions without managing credentials, fallback logic, or rate limiting."
  },
  "gaps": [
    {
      "id": "GAP-001",
      "title": "30+ hardcoded paths in existing implementations",
      "severity": "critical",
      "files": [
        "backend/app/services/implementation_executor.py",
        "backend/app/services/roundtable/validation.py",
        "backend/app/services/evidence_manager.py"
      ]
    },
    {
      "id": "GAP-002",
      "title": "Project-specific logic scattered throughout",
      "severity": "critical",
      "description": "References to 'summitflow' and 'portfolio-ai' hardcoded in tool definitions, validation, and defaults"
    },
    {
      "id": "GAP-003",
      "title": "No retry logic in any client",
      "severity": "high",
      "description": "Single failure = exception. No exponential backoff or rate limit handling."
    },
    {
      "id": "GAP-004",
      "title": "Class-level mutable state in GeminiClient",
      "severity": "medium",
      "description": "_session_service and _active_sessions shared across instances, potential race conditions"
    },
    {
      "id": "GAP-005",
      "title": "Credential management is ad-hoc",
      "severity": "high",
      "description": "Some use CLI auth, some clear ANTHROPIC_API_KEY, no centralized store"
    }
  ],
  "requirements": {
    "goals": [
      {
        "goal": "Project-agnostic service with zero hardcoded paths or project references",
        "criteria": [
          "grep -r 'summitflow|portfolio-ai' src/ returns nothing",
          "grep -r '/home/' src/ returns nothing",
          "All configuration via environment variables or config file"
        ],
        "priority": "must"
      },
      {
        "goal": "Unified API for all AI agent interactions",
        "criteria": [
          "REST endpoints: /complete, /stream, /sessions, /status",
          "WebSocket endpoint for real-time streaming",
          "Consistent request/response format across Claude and Gemini"
        ],
        "priority": "must"
      },
      {
        "goal": "Centralized credential management",
        "criteria": [
          "Credentials stored in PostgreSQL (encrypted)",
          "CRUD via UI settings page",
          "Support for Claude OAuth and Gemini API keys"
        ],
        "priority": "must"
      },
      {
        "goal": "Automatic fallback with configurable rules",
        "criteria": [
          "Primary model fails -> automatic fallback to secondary",
          "Fallback rules configurable per scenario",
          "Cost tracking logs which model actually served request"
        ],
        "priority": "must"
      },
      {
        "goal": "Full observability",
        "criteria": [
          "/health endpoint with provider status",
          "/metrics endpoint (Prometheus format)",
          "/status endpoint with detailed diagnostics"
        ],
        "priority": "must"
      },
      {
        "goal": "Management UI with dashboard and interactive chat",
        "criteria": [
          "Standalone web app (separate port)",
          "Dashboard: active sessions, costs, errors, metrics",
          "Interactive chat: single agent and roundtable mode",
          "Settings: credential CRUD, fallback rules"
        ],
        "priority": "must"
      },
      {
        "goal": "Per-request cost tracking",
        "criteria": [
          "Log model, tokens, estimated cost for every request",
          "Aggregate by project, time period",
          "Dashboard visualization"
        ],
        "priority": "must"
      },
      {
        "goal": "Session persistence across restarts",
        "criteria": [
          "Sessions, messages, context stored in PostgreSQL",
          "SDK session IDs preserved for context resumption",
          "Configurable logging detail per session"
        ],
        "priority": "must"
      },
      {
        "goal": "Tool support via native SDK hooks",
        "criteria": [
          "Claude: PreToolUse/PostToolUse hooks",
          "Gemini: before_tool_callback/after_tool_callback",
          "Permission model: global YOLO mode or tool-level granularity"
        ],
        "priority": "must"
      },
      {
        "goal": "Event publishing for external integrations",
        "criteria": [
          "WebSocket events for UI real-time updates",
          "Webhook callbacks for backend services (memory system)",
          "Events: session_start, tool_use, complete, error"
        ],
        "priority": "must"
      },
      {
        "goal": "Rate limit handling with queue and backoff",
        "criteria": [
          "Queue requests when rate limited",
          "Exponential backoff with configurable parameters",
          "Fallback to alternate provider if both limited"
        ],
        "priority": "should"
      }
    ]
  },
  "decisions": [
    {
      "id": "ARCH-001",
      "title": "Standalone service with own database",
      "context": "Agent Hub needs to be portable across projects and cleanly separated from consumer internals",
      "alternatives": [
        {
          "option": "Integrated into SummitFlow",
          "pros": [
            "Faster to build"
          ],
          "cons": [
            "Tight coupling",
            "Not portable"
          ]
        },
        {
          "option": "Separate package, same deployment",
          "pros": [
            "Middle ground"
          ],
          "cons": [
            "Still coupled"
          ]
        },
        {
          "option": "Standalone service",
          "pros": [
            "Clean interfaces",
            "Portable",
            "Own lifecycle"
          ],
          "cons": [
            "More infrastructure"
          ]
        }
      ],
      "decision": "Standalone service with own PostgreSQL database",
      "rationale": "Maximizes portability and enforces clean boundaries. Other projects (portfolio-ai, future projects) can use it without SummitFlow dependency.",
      "consequences": [
        "Need to set up separate deployment",
        "Database migrations are independent"
      ]
    },
    {
      "id": "ARCH-002",
      "title": "PostgreSQL credential storage with UI CRUD",
      "context": "Need centralized credential management that's accessible and secure",
      "alternatives": [
        {
          "option": "Rely on CLI auth",
          "pros": [
            "Simpler"
          ],
          "cons": [
            "No visibility",
            "User-specific"
          ]
        },
        {
          "option": "Hub manages credentials",
          "pros": [
            "Centralized",
            "Auditable"
          ],
          "cons": [
            "More complexity"
          ]
        },
        {
          "option": "Both supported",
          "pros": [
            "Flexible"
          ],
          "cons": [
            "Two systems"
          ]
        }
      ],
      "decision": "PostgreSQL storage with UI settings page for CRUD",
      "rationale": "Following portfolio-ai's source_credentials pattern. Enables multi-user setups and audit trails.",
      "consequences": [
        "Need encryption at rest",
        "UI must handle secrets securely"
      ]
    },
    {
      "id": "ARCH-003",
      "title": "Native SDK tool calling (not custom JSON protocol)",
      "context": "Both Claude SDK and Google ADK support native tool calling with hooks",
      "alternatives": [
        {
          "option": "Custom JSON protocol",
          "pros": [
            "Works with any provider"
          ],
          "cons": [
            "Deprecated",
            "Fragile parsing"
          ]
        },
        {
          "option": "Native SDK hooks",
          "pros": [
            "Robust",
            "Proper permissions",
            "Streaming"
          ],
          "cons": [
            "Provider-specific code"
          ]
        },
        {
          "option": "Callback-based",
          "pros": [
            "Decoupled"
          ],
          "cons": [
            "Extra round-trips"
          ]
        }
      ],
      "decision": "Native SDK tool calling with PreToolUse/before_tool_callback hooks",
      "rationale": "Most robust and maintainable. The SDK handles edge cases we'd have to reimplement.",
      "consequences": [
        "Need adapter layer per provider",
        "Permission hooks are provider-specific"
      ]
    },
    {
      "id": "ARCH-004",
      "title": "Big bang migration (not incremental)",
      "context": "Existing projects are partially working, not fully production",
      "alternatives": [
        {
          "option": "Incremental",
          "pros": [
            "Safe",
            "Gradual"
          ],
          "cons": [
            "Longer timeline",
            "Maintaining two systems"
          ]
        },
        {
          "option": "Big bang",
          "pros": [
            "Clean cut",
            "Faster"
          ],
          "cons": [
            "Higher risk"
          ]
        },
        {
          "option": "Parallel",
          "pros": [
            "Can compare"
          ],
          "cons": [
            "Double resources"
          ]
        }
      ],
      "decision": "Big bang - build Agent Hub, switch all consumers at once",
      "rationale": "User confirmed breaking existing functionality is acceptable since nothing is fully production yet.",
      "consequences": [
        "Need comprehensive testing before switch",
        "Old code can be deleted immediately after"
      ]
    },
    {
      "id": "ARCH-005",
      "title": "Both WebSocket and REST APIs",
      "context": "Need real-time streaming for UI and simple calls for automation",
      "alternatives": [
        {
          "option": "REST only",
          "pros": [
            "Simple"
          ],
          "cons": [
            "No streaming"
          ]
        },
        {
          "option": "WebSocket only",
          "pros": [
            "Unified"
          ],
          "cons": [
            "Overkill for simple calls"
          ]
        },
        {
          "option": "Both",
          "pros": [
            "Flexible"
          ],
          "cons": [
            "Two interfaces to maintain"
          ]
        }
      ],
      "decision": "REST for simple requests, WebSocket for streaming and interactive sessions",
      "rationale": "REST is simpler for automation (Code Health Agent). WebSocket essential for UI and long-running tasks.",
      "consequences": [
        "Need to maintain both interfaces",
        "Session state must work across both"
      ]
    },
    {
      "id": "ARCH-006",
      "title": "Frontend stack determined by frontend-design plugin",
      "context": "User wants high-quality UI design",
      "alternatives": [
        {
          "option": "Next.js + Tailwind",
          "pros": [
            "Match SummitFlow"
          ],
          "cons": [
            "Might not be optimal"
          ]
        },
        {
          "option": "Vite + React",
          "pros": [
            "Faster builds"
          ],
          "cons": [
            "Different from existing"
          ]
        },
        {
          "option": "frontend-design plugin choice",
          "pros": [
            "Optimized for design quality"
          ],
          "cons": [
            "Unknown until plugin runs"
          ]
        }
      ],
      "decision": "Use frontend-design plugin for all UI/UX decisions",
      "rationale": "User explicitly requested this. Plugin will recommend optimal stack for design quality.",
      "consequences": [
        "Frontend stack TBD until plugin runs",
        "Must invoke plugin for all UI work"
      ]
    }
  ],
